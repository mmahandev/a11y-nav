{
  "name": "a11y-nav",
  "version": "1.0.1",
  "description": "Library for accessible navigations",
  "author": "mmahandev",
  "license": "MIT",
  "source": "src/a11y-nav.ts",
  "scripts": {
    "dev": "vite",
    "build": "microbundle --name A11YNav --sourcemap false && cp src/a11y-nav.css dist/",
    "build:web": "vite build",
    "prebuild": "rimraf dist",
    "preview": "vite preview"
  },
  "exports": {
    "require": "./dist/a11y-nav.cjs",
    "default": "./dist/a11y-nav.modern.js"
  },
  "main": "./dist/a11y-nav.cjs",
  "module": "./dist/a11y-nav.module.js",
  "unpkg": "dist/a11y-nav.umd.js",
  "style": "dist/a11y-nav.css",
  "devDependencies": {
    "@types/prismjs": "^1.26.0",
    "babel-plugin-prismjs": "^2.1.0",
    "microbundle": "^0.14.2",
    "prismjs": "^1.28.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.5",
    "vite-plugin-prismjs": "^0.0.8"
  }
}
